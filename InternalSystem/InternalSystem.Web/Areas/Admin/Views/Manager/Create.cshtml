@model InternalSystem.Core.Models.Manager
@{
    Layout = "~/areas/Admin/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "添加管理员";
}
<div class="row" ng-controller="PackageController">
    <div class="col-md-12">
        <div class="portlet light">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-cogs font-green-sharp"></i>
                    <span class="caption-subject font-green-sharp bold uppercase">@ViewBag.Title</span>
                </div>
            </div>
            <div class="portlet-body form">
                @using (Html.BeginForm("Create", "Manager", FormMethod.Post, new { @class = "form-horizontal", @role = "form", enctype = "multipart/form-data" }))
                {
                    <input type="hidden" value="@(ViewBag.userType)" name="userType" id="userType" />
                    <div class="form-body">
                        <div class="form-group">
                            <label class="col-md-3 control-label">UserName</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Name, new { @class = "form-control", placeholder = "UserName" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Password</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Pass, new { @class = "form-control", placeholder = "Password" })
                                <span id="hascode" style="color:red"></span>
                            </div>
                        </div>

                        <div class="form-actions">
                            <div class="row">
                                <div class="col-md-offset-3 col-md-9">
                                    <button type="submit" class="btn green" id="sub">保 存</button>
                                    <a href="@Url.Action("List")" cl class="btn default">取消，返回列表</a>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery.js"></script>
<script>

</script>