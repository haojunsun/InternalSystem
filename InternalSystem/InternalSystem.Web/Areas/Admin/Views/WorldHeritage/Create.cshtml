                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-md-3 control-label"> 备注：</label>
                                <div class="col-md-6">
                                    @Html.TextAreaFor(m => m.Remarks, new { @class = "form-control", placeholder = " 备注", rows = "10" })
                                </div>
                            </div>

                            <div class="form-group">
