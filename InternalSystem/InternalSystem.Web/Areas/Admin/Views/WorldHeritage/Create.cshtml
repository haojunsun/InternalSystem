    <script type="text/javascript">
        selectType = function (ele) {
            var typeindex = $(ele).val();
            if (typeindex == 0) {
                $('.videodiv').css('display', 'block');
                $('.picdiv').css('display', 'none');
                $('.worddiv').css('display', 'none');
            } else if (typeindex == 1) {
                $('.worddiv').css('display', 'block');
                $('.videodiv').css('display', 'none');
                $('.picdiv').css('display', 'none');
            } else if (typeindex == 2) {
                $('.picdiv').css('display', 'block');
                $('.videodiv').css('display', 'none');
                $('.worddiv').css('display', 'none');
            }
        }

        $(function () {
            forup();

            $('.videodiv').css('display', 'none');
            $('.picdiv').css('display', 'none');
            $('.worddiv').css('display', 'none');

            $('#hascode').text('');
            $("#sub").attr("disabled", true);
            $('#rgcode').blur(function () {
                //值相等，不重复发送请求
                if (rgcode != $(this).val() && $(this).val() != '') {
                    rgcode = $(this).val(); //人工编码
                    $.get("HasCode?code=" + rgcode, function (data) {
                        if (data != "") {
                            $('#hascode').text('人工编码已存在');
                            $("#sub").attr("disabled", true);
                        } else {
                            $('#hascode').text('');
                            $("#sub").attr("disabled", false);
                        }
                    });
                } else if ($(this).val() == '') {
                    $('#hascode').text('人工编码不能为空');
                    $("#sub").attr("disabled", true);
                }
            });
        });
        var swfu;
        //上传模型部分
        function forup() {
            swfu = new SWFUpload({
                upload_url: "/WorldHeritage/Upload",
                flash_url: "/Scripts/SwfUpload/Flash/swfupload.swf",
                file_size_limit: "2000000",//最大上传文件
                file_types: "*.*",
                file_types_description: "All Files",
                file_upload_limit: 0,
                file_queue_limit: 3,
                debug: false,

                // Button settings

                button_width: "165",
                button_height: "29",
                button_placeholder_id: "spanButtonPlaceHolder",
                button_text: '<span class="theFont">点击选择文件</span>',
                button_text_style: ".theFont { font-size: 16; }",
                button_text_left_padding: 10,
                button_text_top_padding: 11,

                file_dialog_complete_handler: function (numFilesSelected, numFilesQueued) {
                },
                file_queued_handler: function (file) {
                    $("#fileName").val(file.name);
                    filenamehere = $("#fileName").val(file.name);
                    $("#FileName").val(file.name);
                    $("#sub").attr("disabled", true);//开始上传 禁用提交按钮
                    console.log(file.name);
                },
                upload_progress_handler: function (file, complete, total) {
                    var value = complete / total * 100;
                    if (value < 100) {
                        $('#p').progressbar('setValue', value.toFixed(2));
                    }
                    $("#CurrentSpeed").html(SWFUpload.speed.formatBPS(file.currentSpeed));
                },
                upload_success_handler: function (file, data) {
                    //大本件object上传成功后提交小文件
                    alert("文件上传成功");
                    $('#progressdiv').css("display", "none");
                    $("#sub").attr("disabled", false);//上传完成 启用提交按钮
                }

            });
            $("#cancelButton").click(function () {
                swfu.stopUpload();
            });
        };
        //提交增加内容
        function clickSubmit() {
            if (swfu.getStats().files_queued > 0) {
                $('#progressdiv').css("display", "block");
                $('#p').progressbar('setValue', 0);
                swfu.startUpload();
            }
        }


    </script>

</head>
