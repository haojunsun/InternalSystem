                                @Html.TextBoxFor(m => m.FirstLevelOfSubjectClassification, new { @class = "form-control", placeholder = "资源属性 一级分类" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">资源属性 二级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.SecondLevelOfResourcesClassification, new { @class = "form-control", placeholder = "资源属性 二级分类" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">资源属性 三级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.ThirdLevelOfResourcesClassification, new { @class = "form-control", placeholder = "资源属性 三级分类" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">资源属性 关键词：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Keywords, new { @class = "form-control", placeholder = "关键词" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">描述 摘要：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Abstract, new { @class = "form-control", placeholder = "描述 摘要" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">描述 获奖 奖项名称：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.NameOfAwards, new { @class = "form-control", placeholder = "描述 获奖 奖项名称" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">描述 获奖 获奖年度或届次：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.YearOrTimeOfAwards, new { @class = "form-control", placeholder = "描述 获奖 获奖年度或届次" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">描述 获奖 颁奖单位：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Awarder, new { @class = "form-control", placeholder = "描述 获奖 颁奖单位" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">类型：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Type, new { @class = "form-control", placeholder = "类型" })
                            </div>
                        </div>



                        <div class="form-group">
                            <label class="col-md-3 control-label">相关人物/组织 人物/组织名称：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Name, new { @class = "form-control", placeholder = "相关人物/组织 人物/组织名称" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">责任方式：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Role, new { @class = "form-control", placeholder = "责任方式" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">民族：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.NationalityOfRelatedPeople, new { @class = "form-control", placeholder = "民族" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">籍贯：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.BirthplaceOfRelatedPeople, new { @class = "form-control", placeholder = "籍贯" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">性别：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.GenderOfRelatedPeople, new { @class = "form-control", placeholder = "性别" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">年龄：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.AgeOfRelatedPeople, new { @class = "form-control", placeholder = "年龄" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">文化程度：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.EducationLevelOfRelatedPeople, new { @class = "form-control", placeholder = "文化程度" })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-md-3 control-label">职业/职称：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.VocationOfRelatedPeople, new { @class = "form-control", placeholder = "职业/职称" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">时间类型：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.TemporalType, new { @class = "form-control", placeholder = "时间类型" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">历史纪年 起始时间：</label>
                            <div class="col-md-4">
