@model  InternalSystem.Core.Models.WorldHeritage
@{
    Layout = "~/areas/Admin/Views/Shared/_Layout.cshtml";
    ViewBag.Title = Model.Title + "-" + "编辑";
}
<link rel="stylesheet" href="~/theme/style.css" type="text/css" media="screen" />
<div class="row" ng-controller="PackageController">
    <div class="col-md-12">
        <div class="portlet light">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-cogs font-green-sharp"></i>
                    <span class="caption-subject font-green-sharp bold uppercase">@Model.Title</span>
                </div>
            </div>
            <div class="portlet-body form">
                @using (Html.BeginForm("Edit", "WorldHeritage", FormMethod.Post, new { @class = "form-horizontal", @role = "form", enctype = "multipart/form-data" }))
                {
                    @Html.HiddenFor(model => model.WorldHeritageId)
                    <div class="form-body">
                        <div class="form-group">
                            <label class="col-md-3 control-label">系统编码：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.InventoryId, new { @class = "form-control", placeholder = "系统编码" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">人工编码：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.ArtificialId, new { @class = "form-control", placeholder = "人工编码", id = "rgcode" })
                                <span id="hascode" style="color:red"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">正题名：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.TitleProper, new { @class = "form-control", placeholder = "正题名" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">其他题名：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.OtherTitle, new { @class = "form-control", placeholder = "其他题名" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术门类 一级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.FirstLevelOfArtClassification, new { @class = "form-control", placeholder = "艺术门类 一级分类" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术门类 二级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.SecondLevelOfArtClassification, new { @class = "form-control", placeholder = "艺术门类 二级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术门类 三级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.ThirdLevelOfArtClassification, new { @class = "form-control", placeholder = "艺术门类 三级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">民族属性 一级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.FirstLevelOfEthnicGroup, new { @class = "form-control", placeholder = "民族属性 一级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label"> 民族属性 二级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.SecondLevelOfEthnicGroup, new { @class = "form-control", placeholder = " 民族属性 二级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">民族属性 三级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Subgroup, new { @class = "form-control", placeholder = "民族属性 三级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">社会属性 一级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.FirstLevelOfSocialAttributes, new { @class = "form-control", placeholder = "社会属性 一级分类" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">社会属性 二级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.SecondLevelOfSocialAttributes, new { @class = "form-control", placeholder = "社会属性 二级分类" })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术形态 时代属性：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.TimesProperty, new { @class = "form-control", placeholder = "艺术形态 时代属性" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术形态  组成要素 一级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.FirstLevelOfElements, new { @class = "form-control", placeholder = "艺术形态  组成要素 一级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术形态 组成要素 二级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.SecondLevelOfElements, new { @class = "form-control", placeholder = "艺术形态 组成要素 二级分类" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术形态 组成要素 三级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.ThirdLevelOfElements, new { @class = "form-control", placeholder = "艺术形态 组成要素 三级分类" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">脚色行当 一级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.FirstLevelOfCharacter, new { @class = "form-control", placeholder = "脚色行当 一级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">脚色行当 二级分类：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.SecondLevelOfCharacter, new { @class = "form-control", placeholder = "脚色行当 二级分类" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术形态 艺术流派：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.ArtSchool, new { @class = "form-control", placeholder = "艺术形态 艺术流派" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">类型：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.PerformingForm, new { @class = "form-control", placeholder = "艺术形态 表演形式" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">艺术形态 结构体式：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Structure, new { @class = "form-control", placeholder = "艺术形态 结构体式" })
                            </div>
                        </div>
