                                @Html.TextBoxFor(m => m.SongPattern, new { @class = "form-control", placeholder = "曲牌板式 曲牌名" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">曲牌板式 板式：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.BeatsPattern, new { @class = "form-control", placeholder = "曲牌板式 板式" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">律吕宫调 民间调式：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.MusicModeName, new { @class = "form-control", placeholder = "律吕宫调 民间调式" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">律吕宫调 工尺谱系：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.GongCheModeName, new { @class = "form-control", placeholder = "律吕宫调 工尺谱系" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label"> 律吕宫调 简谱系统：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.NumberedMusicalNotationModeName, new { @class = "form-control", placeholder = " 律吕宫调 简谱系统" })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-md-3 control-label">律吕宫调 《九宫大成》系统：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.JiuGongDaChengModeName, new { @class = "form-control", placeholder = "律吕宫调 《九宫大成》系统" })
