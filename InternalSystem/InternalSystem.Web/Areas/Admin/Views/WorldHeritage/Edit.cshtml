                        <div class="form-group">
                            <label class="col-md-3 control-label">资料格式类型：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.ReferencesFormat, new { @class = "form-control", placeholder = "资料格式类型" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">获取方式：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.AcquisitionMethod, new { @class = "form-control", placeholder = "获取方式" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">源资料提供者：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.ProviderOfReferences, new { @class = "form-control", placeholder = "源资料提供者" })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-md-3 control-label">源资料典藏单位：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.RepositoryName, new { @class = "form-control", placeholder = "源资料典藏单位" })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-md-3 control-label">出版 出版者：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Publisher, new { @class = "form-control", placeholder = "出版 出版者" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">出版 出版时间：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.PublicationDate, new { @class = "form-control", placeholder = "出版 出版时间" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">出版 出版地：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.PlaceOfPublication, new { @class = "form-control", placeholder = "出版 出版地" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">数字化信息  数字化格式：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.DigitalObjectFormat, new { @class = "form-control", placeholder = "数字化信息  数字化格式" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">大小：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Size, new { @class = "form-control", placeholder = "大小" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">时长：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Duration, new { @class = "form-control", placeholder = "时长" })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-md-3 control-label">分辨率（解析度）：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.DigitalSpecification, new { @class = "form-control", placeholder = "分辨率（解析度）" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">音/视频数据码率：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.AudioVideoBitRate, new { @class = "form-control", placeholder = "音/视频数据码率" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">音频采样频率：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.AudioSamplingFrequency, new { @class = "form-control", placeholder = "音频采样频率" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">声道数：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.NumberOfChannels, new { @class = "form-control", placeholder = "声道数" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">储存地址：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.FilePath, new { @class = "form-control", placeholder = "储存地址" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">显示级别：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.DisplayType, new { @class = "form-control", placeholder = "显示级别" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">备份光盘存放编号：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.CDNumber, new { @class = "form-control", placeholder = "备份光盘存放编号" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">授权者：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Holder, new { @class = "form-control", placeholder = "授权者" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">授权起止时间：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.RightsDate, new { @class = "form-control", placeholder = "授权起止时间" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">授权条款：</label>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Licens, new { @class = "form-control", placeholder = "授权条款" })
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">备注：</label>
                            <div class="col-md-6">
                                @Html.TextAreaFor(m => m.Note, new { @class = "form-control", placeholder = " 备注", rows = "10" })
                            </div>
                        </div>
                        @if (Model.HeritageType == 2)
                        {
                            <div class="form-group">
                                <label class="col-md-3 control-label">图片：</label>
                                <div class="col-md-9">
                                    <img src="~/Uploads/pic/@(Model.FileName)" alt="" width="300px" />
                                    <div class="fileinput fileinput-new" data-provides="fileinput">
                                        <div>
                                            <span class="btn default btn-file">
                                                <span class="fileinput-new">
                                                    选择图片
                                                </span>
                                                <input type="file" name="file">
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                        else if (Model.HeritageType == 0)
                        {
                            <input type="hidden" value="@(Model.FileName)" id="videourl" />
                            <div class="form-group">
                                <label class="control-label col-md-1" style="width:200px">媒体文件:</label>
                                <div class="">
                                    <center>
                                        <div>
                                            <video id="videoplayer" poster="~/img/pic.png" title="this is Projekktor" width="900" height="500" controls=controls>
                                                <source id="videosource" src="" type="video/mp4" />
                                            </video>
                                        </div>
                                    </center>
                                </div>
                            </div>
                        }
                        else if (Model.HeritageType == 1)
                        {
                            <div class="form-group">
                                <label class="control-label col-md-1" style="width:200px">内容:</label>
                                <div class="col-md-6">
                                    @Html.TextAreaFor(m => m.Content, new { @class = "form-control", placeholder = "内容", rows = "20" })
                                </div>
                            </div>
                        }
                        <div class="form-actions">
                            <div class="row">
                                <div class="col-md-offset-3 col-md-9">
                                    <button type="submit" class="btn green">保 存</button>
                                    <a href="@Url.Action("My")" class="btn default">取消，返回列表</a>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery.js"></script>
<script src="~/Scripts/projekktor.js"></script>
<script>
    $(function () {
        var curWwwPath = window.document.location.href;
        var pathName = window.document.location.pathname;
        var pos = curWwwPath.indexOf(pathName);
        var localhostPaht = curWwwPath.substring(0, pos);

        $('#videosource').attr("src", localhostPaht + $('#videourl').val().substring(1));
        setTimeout(function () {
            projekktor('#videoplayer'); // instantiation
        }, 100)
    })
</script>