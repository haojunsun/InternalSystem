@model List<PatientInformation.Core.Models.Article>

<div class="portlet-title">
    <div class="caption">
     
        <span class="caption-subject font-green-sharp bold uppercase">@(ViewBag.CurrentTime)</span>
    </div>
    <div class="pull-right">

    
     
       
    </div>
</div>
<div class="table-scrollable table-scrollable-borderless">
    
    <table class="table table-hover table-light">
        <thead>
            <tr>
                <th>发布人</th>
                <th>标题</th>
                <th>频道</th>
                <th>栏目</th>
                <th>开始日期</th>

            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.Count != 0)
            {
                foreach (var item in Model.Where(item => item != null))
                {
                    <tr>
                        <td>
                            @item.ManagerName
                        </td>
                        <td>
                            @item.Title
                        </td>
                        <td>
                            @item.ChannelTags
                        </td>
                        <td>
                            @item.ColumnTags
                        </td>
                        <td>
                            @DateTime.Parse(item.CreatedUtc.ToString()).ToString("yyyy-MM-dd HH:mm:ss")
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
